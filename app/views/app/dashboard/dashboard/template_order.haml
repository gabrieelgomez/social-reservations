!!!
%html{:lang => "en"}
  %head
    = render 'app/dashboard/dashboard/layouts/head'
    -#= render 'app/layouts/head'
  %body
    -#.loader
      = render 'app/front/loader/loader'
    %div#js-mobile
      .notifications
        %span{:mode => "in-out"}
      .wrapper
        .sidebar{style: "background-image: url(/assets/img/sidebar-5.jpg)"}
          = render 'app/dashboard/dashboard/layouts/sidebar'
        .main-panel
          = render 'app/dashboard/dashboard/layouts/main'
          .content
            .container-fluid
              .row
                .col-12
                  .card
                    / 
                    .card-header
                      %h4.card-title Traslados
                      %p.card-category Lorem ipsum dolor sit amet consectetur adipisicing elit.
                    .card-body
                      .table-responsive
                        %table.table.table-hover.table-striped
                          %thead
                            %th N° Orden
                            %th N° Factura
                            %th Moneda
                            %th Costo 
                            %th Estatus 
                            %th Pago
                            %th Vehiculo
                            %th Origen
                            %th Destino
                            %th Pasajeros
                          %tbody
                            %tr
                              %td 1
                              %td 77712
                              %td USD
                              %td 90.000000,00
                              %td Pendiente
                              %td Pendiente
                              %td Vans 
                              %td Aeropuerto El Dorado
                              %td Hotel Colombia
                              %td 4
      
    = render 'app/dashboard/dashboard/layouts/scripts'








