#modalPrivacyPolicy.modal.fade{:role => "dialog"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{"data-dismiss" => "modal", :type => "button"} ×
        %h4.modal-title Políticas de Privacidad
      .modal-body
        %p= @setting.try("privacy_policies_#{@lang}".to_sym).try(:html_safe)
      .modal-footer
        %button.btn.btn-default{"data-dismiss" => "modal", :type => "button"} Close
