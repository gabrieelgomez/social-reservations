- objects.each do |agent|
  %tr{ id: "row-#{agent.id}" }
    %td.item-check{ style: 'width: 10%' }
      - if can?(KepplerTravel::Agent).destroy_multiple?
        .checkbox
          %label{ for: "checkbox-#{agent.id}" }
            %input{ type:"checkbox", id: "checkbox-#{agent.id}", 'checklist-model' => 'check', 'checklist-value' => "#{agent.id}", '@click' => "listDelete(#{agent.id})"}
            %span.cr
              %i.cr-icon.glyphicon.glyphicon-ok
    %td.hidden-xs
      = link_to admin_travel_agent_path(agent, search: @query), remote: true, class: 'show-row' do
        %span
          = agent.unique_code
    %td.hidden-xs
      = link_to admin_travel_agent_path(agent, search: @query), remote: true, class: 'show-row' do
        %span
          = agent.comission_percentage
    %td.hidden-xs.hidden-sm
      = link_to admin_travel_agent_path(agent, search: @query), remote: true, class: 'show-row' do
        %span
          = agent.lending_percentage
    -# %td.hidden-xs.hidden-sm
    -#   = link_to admin_travel_agent_path(agent, search: @query), remote: true, class: 'show-row' do
    -#     %span
    -#       = agent.user
    %td.hidden-xs.hidden-sm
      = link_to admin_travel_agent_path(agent, search: @query), remote: true, class: 'show-row' do
        %span
          = agent.agency
    %td{ style: 'width: 15px;padding-right:15px;' }
      %a.action-btn.pull-right{:id => "menu-#{agent.id}", "aria-expanded" => "false", "aria-haspopup" => "true", "data-toggle" => "dropdown"}
        %i.icon-options-vertical
      %ul.actions.dropdown-menu{"aria-labelledby" => "menu-#{agent.id}" }
        - if can?(KepplerTravel::Agent).index?
          %li
            = link_to admin_travel_agent_path(agent), class: 'new-menu', title: t('keppler.actions.show') do
              %i.icon-eye
              = t('keppler.actions.show')
        - if can?(KepplerTravel::Agent).update?
          %li
            = link_to edit_admin_travel_agent_path(agent), class: 'new-menu', title: t('keppler.actions.edit') do
              %i.icon-pencil
              = t('keppler.actions.edit')
        -# - if can?(KepplerTravel::Agent).clone?
        -#   %li
        -#     = link_to admin_travel_agent_clone_path(agent), class: 'new-menu', title: t('keppler.actions.clone') do
        -#       %i.icon-docs
        -#       = t('keppler.actions.clone')
        - if can?(KepplerTravel::Agent).destroy?
          %li
            = link_to admin_travel_agent_path(agent), class: 'new-menu', title: t('keppler.actions.delete'), method: :delete, data: { confirm: t("keppler.messages.data_confirm") } do
              %i.icon-trash
              = t('keppler.actions.delete')

    %td{:style => 'width: 5%;'}
      -# %a.drop{:href=>"#"}
      -#   %i.icon-cursor-move
